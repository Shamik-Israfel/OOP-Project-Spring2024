<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coach.OrderEquipmentsController">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Equipment Name:" />
        <Label layoutX="50.0" layoutY="100.0" text="Price:" />
        <Label layoutX="50.0" layoutY="150.0" text="Quantity Available:" />
        <Label layoutX="50.0" layoutY="200.0" text="Specifications:" />
        
        <TextField layoutX="200.0" layoutY="50.0" prefWidth="200.0" />
        <TextField layoutX="200.0" layoutY="100.0" prefWidth="200.0" />
        <TextField layoutX="200.0" layoutY="150.0" prefWidth="200.0" />
        <TextField layoutX="200.0" layoutY="200.0" prefWidth="200.0" />
        
        <Button layoutX="50.0" layoutY="250.0" mnemonicParsing="false" onAction="#orderEquipmentOnMouseClicked" text="Order Equipment" />
        <Button layoutX="200.0" layoutY="250.0" mnemonicParsing="false" onAction="#cancelOrderOnMouseClicked" text="Cancel Order" />
      <Button layoutX="447.0" layoutY="345.0" mnemonicParsing="false" onAction="#goBackOnMouseClicked" text="Go Back" />
    </children>
</AnchorPane>
