<AnchorPane prefHeight="445.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sponsor.Dashboard_sponsorController">
   
    <!-- Financial Support Tracker Button -->
    <Button layoutX="188.0" layoutY="65.0" mnemonicParsing="false" onAction="#financialSupportTrackerOnMouseClicked" style="-fx-background-color: pink;" text="Financial Support Tracker" />

